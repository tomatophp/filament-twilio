{
    "name": "tomatophp/filament-twilio",
    "type": "library",
    "description": "Send Whatsapp messages using Twilio and native filament Notification Facade class",
    "keywords": [
        "php",
        "laravel",
        "twilio",
        "whatsapp",
        "notification",
        "filament-plugin",
        "filament"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\FilamentTwilio\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\FilamentTwilio\\FilamentTwilioServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "tomatophp/console-helpers": "^1.1",
        "twilio/sdk": "^8.2",
        "filament/filament": "^3.0",
        "filament/notifications": "^3.0"
    }
}
